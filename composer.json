
{
    "name": "dratejinn/telegrambot",
    "description": "A Telegram bot api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek van der Veer",
            "email": "freek_vanderveer@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "monolog/monolog": "^3.0",
        "robmorgan/phinx": "^0.8.1"
    },
    "suggest": {
        "robmorgan/phinx": "Easy database migrations and an applicable migration exists for existing objects",
        "monolog/monolog": "Useful for logging purposes"
    },
    "autoload": {
        "psr-4": {
            "Telegram\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Examples\\":"Examples/"
        }
    }
}
