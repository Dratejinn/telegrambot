{
    "name": "dratejinn/telegrambot",
    "description": "A Telegram bot api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek van der Veer",
            "email": "freek_vanderveer@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-curl": "*",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "robmorgan/phinx": "^0.8.1"
    },
    "suggest": {
        "robmorgan/phinx": "Allow easy use for mysql storage migrations",
        "monolog/monolog": "Allow great logging"
    },
    "autoload": {
        "psr-4": {
            "Telegram\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Examples\\":"Examples/"
        }
    }
}
